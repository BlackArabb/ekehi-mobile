<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">fra.cloud.appwrite.io</domain>
        <domain includeSubdomains="true">yourdomain.com</domain> <!-- Replace with your actual domain -->
    </domain-config>
    
    <!-- Allow cleartext traffic for all domains (for development only) -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>