{
  "name": "ekehi-network-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 3000",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "build": "expo export --platform web",
    "vercel-build": "expo export --platform web",
    "export-cloudflare-data": "node export-cloudflare-data.js",
    "migrate-data": "node migrate-data.js",
    "test-appwrite": "node test-appwrite.js",
    "wrangler-export": "npx wrangler d1 export ekehi-network-db --remote --output ./cloudflare-data/ekehi-network-db.sql",
    "setup-cloudflare": "npx wrangler auth login && npx wrangler d1 list",
    "setup-collections": "node setup-appwrite-collections.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/vector-icons": "^14.0.2",
